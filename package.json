{
  "name": "contapyme-sistema-contable",
  "version": "1.0.0",
  "description": "Sistema contable integral para PyMEs con dashboard, balances y proyecciones financieras",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run type-check && npm run lint:strict && npm run format:check",
    "quality:fix": "npm run format && npm run lint:fix && npm run type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "db:types": "supabase gen types typescript --project-id YOUR_PROJECT_ID --schema public > src/types/supabase.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "audit:security": "npm audit --audit-level moderate",
    "audit:deps": "npm outdated",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "dev:debug": "NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@hookform/resolvers": "^3.3.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.38.0",
    "canvas": "^3.1.2",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "exceljs": "^4.4.0",
    "form-data": "^4.0.4",
    "html2canvas": "^1.4.1",
    "jsdom": "^26.1.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.10.38",
    "qrcode": "^1.5.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.45.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-extended": "^7.0.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.0",
    "prettier": "^3.6.2",
    "supabase": "^1.112.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "contabilidad",
    "pymes",
    "finanzas",
    "dashboard",
    "typescript",
    "nextjs"
  ],
  "author": "Contapyme Team",
  "license": "MIT"
}
